<templateSet group="Rust">
  <template name="p" value="println!(&quot;_____________'$EXPR2$' = '{:?}'_____________&quot;, $EXPR$);$END$" description="println!" toReformat="true" toShortenFQNames="true">
    <variable name="EXPR" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="EXPR2" expression="escapeString(EXPR)" defaultValue="" alwaysStopAt="false" />
  </template>
  <template name="l" value="log::info!(&quot;_____________'$EXPR2$' = '{}'_____________&quot;, $EXPR$);$END$" description="li" toReformat="true" toShortenFQNames="true">
    <variable name="EXPR" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="EXPR2" expression="escapeString(EXPR)" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="RUST_STATEMENT" value="true" />
    </context>
  </template>
  <template name="," value="::$1$" description="" toReformat="false" toShortenFQNames="true">
    <variable name="1" expression="complete()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="ma" value="match $1$ {&#10;    Ok(success) =&gt; {&#10;        $2$&#10;    }&#10;    Err(error) =&gt; {&#10;        log::error!(&quot;_____________'e' = '{}'_____________&quot;, error);&#10;    }&#10;}" description="match" toReformat="false" toShortenFQNames="true">
    <variable name="1" expression="completeSmart()" defaultValue="" alwaysStopAt="true" />
    <variable name="2" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="RUST_FILE" value="true" />
    </context>
  </template>
  <template name="ff" value="for i in 0..$1$ {&#10;   $2$ &#10;}" description="" toReformat="false" toShortenFQNames="true">
    <variable name="1" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="2" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="RUST_FILE" value="true" />
    </context>
  </template>
  <template name="m2" value="match $1$ {&#10;    Some(success) =&gt; {&#10;        $2$;&#10;    }&#10;    None =&gt; {&#10;        None;&#10;    }&#10;}" description="match" toReformat="false" toShortenFQNames="true">
    <variable name="1" expression="completeSmart()" defaultValue="" alwaysStopAt="true" />
    <variable name="2" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="RUST_FILE" value="true" />
    </context>
  </template>
</templateSet>